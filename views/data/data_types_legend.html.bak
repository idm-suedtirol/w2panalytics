{{ul_id = "%s_list_%s" %(tab_name, station)}}
<div class="well panel">
<div class="container-title-sidebar">
<h3 class="title-sidebar"><a data-toggle="collapse" class='link_stations' data-parent="#sidebar_{{=tab_name}}" href="#{{=ul_id}}">{{=name}}</a></h3>
</div>
<div class="collapse in" id="{{=ul_id}}">
<ul class="load_data nav nav-list" data-station="{{=station}}">
        {{for data_type in data_types:}}
            <li><a  id='{{=IS_SLUG()("type_%s_%s_%s" % (station,data_type[0],data_type[3]))[0]}}' title="{{="%s (%s)s" % (data_type[2],data_type[3]) if len(data_type) > 3 else (data_type[2] or data_type[0])}}" data-type='{{=data_type[0]}}' {{=XML('data-period="%s"' % data_type[3]) if len(data_type) > 3 else ''}} data-frontend="{{=frontend}}" data-station="{{=station}}" data-unit="{{=data_type[1]}}" href="#" class="muted" data-station-name="{{=name}}">
            <i class="fa fa-square-o" id="square" onclick="active_link(this)"></i>
            {{="%s %s" % (data_type[0], ("(%s)" % data_type[1]) if data_type[1] != '' else '') if len(data_type) > 3 else (data_type[0])}}
            </a></li>    
        {{pass}}
    </ul>
</div>
</div>
